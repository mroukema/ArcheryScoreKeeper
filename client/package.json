  },
  "homepage": "https://github.com/mroukema/ArcheryScoreKeeper#readme",
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.2",
    "elm-live": "^4.0.1",
    "@nestjs/cli": "^7.5.4"
  }
}
